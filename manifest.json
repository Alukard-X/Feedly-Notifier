{
  "manifest_version": 2,

  "name": "Feedly notifier",
  "description": "Extension for Feedly news",
  "version": "0.1",

  "permissions": [
	"storage",
	"tabs",
    "http://cloud.feedly.com/"
  ],
  "browser_action": {
    "default_icon": "/images/icon.png"
  },
  "background": {
    "page": "default.html"
  },
  "icons": {
      "16": "/images/icon16.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
  }
}