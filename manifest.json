{
  "manifest_version": 2,

  "name": "Feedly notifier",
  "description": "Extension for Feedly news",
  "version": "0.1",

  "permissions": [
	"storage",
	"tabs",
    "http://cloud.feedly.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "page": "default.html"
  }
}